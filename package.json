    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-toast": "^1.2.15",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "class-variance-authority": "^0.7.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.41.0",
    "next": "^15.2.3",
    "next-auth": "^4.24.7",
    "node-cron": "^4.2.1",
    "pg-boss": "^10.1.6",
    "postgres": "^3.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.4.0",
    "ts-results-es": "^6.0.0",
    "tsx": "^4.20.6",
    "zod": "^3.24.2"
