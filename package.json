{
  "name": "football-signup",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:script": "tsc entrypoint.ts --outDir dist",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "dotenv -e .env -- tsx entrypoint.ts",
    "dev:cli": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "dotenv -e .env -- node dist/entrypoint.js",
    "start2": "yarn run build:script && dotenv -e .env -- node dist/entrypoint.js",
    "start:cli": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/core": "^0.34.3",
    "@auth/drizzle-adapter": "^1.7.2",
    "@emotion/react": "^11.14.0",
